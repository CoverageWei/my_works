select term_web_unit_finished_count, term_web_unit_total_count, ceiling((term_web_unit_finished_count / term_web_unit_total_count) * 100), (term_web_unit_finished_count / term_web_unit_total_count), (term_web_unit_finished_count / term_web_unit_total_count) >= 1 from qyy_mid_online.term_learn_progress_stat where day="2017-06-29" and member_id = 1026726748 and term_id=400000001059014 ;

