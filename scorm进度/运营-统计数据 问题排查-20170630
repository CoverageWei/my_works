select * from qyy_mid_online.term_learn_progress_stat_kv where day="2017-06-29" and member_id =1014361903 and term_id=400000000382002;




select term_web_unit_finished_count, term_web_unit_total_count, ceiling((term_web_unit_finished_count / term_web_unit_total_count) * 100) from qyy_mid_online.term_learn_progress_stat where day="2017-06-29" and member_id =1014361903 and term_id=400000000382002;
NULL	4	NULL

select term_web_unit_finished_count, term_web_unit_total_count, ceiling((term_web_unit_finished_count / term_web_unit_total_count) * 100)>99, if(ceiling((term_web_unit_finished_count / term_web_unit_total_count) * 100)>99, 0, 1)  from qyy_mid_online.term_learn_progress_stat where day="2017-06-29" and member_id =1014361903 and term_id=400000000382002;




select tt1.member_id, tt1.term_id, 'user_web_learn_progress',
              if(cast(tt1.term_web_unit_total_count as bigint) >= 1,
                  if(cast(tt1.term_web_unit_total_count as bigint) = 1 and cast(tt1.scorm_unit_sum as bigint) = 1,
                      ceiling(if(t6.view_progress is null, 0, t6.view_progress) * 100),
                      if(cast(tt1.term_web_unit_finished_count as bigint) >= 0,
                          if((tt1.term_web_unit_finished_count / tt1.term_web_unit_total_count) * 100 <= 0,
                              0,
                              if((tt1.term_web_unit_finished_count / tt1.term_web_unit_total_count) * 100 <= 99,
                                  ceiling((tt1.term_web_unit_finished_count / tt1.term_web_unit_total_count) * 100),
                                  if((tt1.term_web_unit_finished_count / tt1.term_web_unit_total_count) * 100 < 100,
                                      floor((tt1.term_web_unit_finished_count / tt1.term_web_unit_total_count) * 100),
                                      100
                                  )
                              )
                          ),
                          0
                      )
                  ),
                  null
              ) as user_web_learn_progress
              from
                  (
                      select t1.member_id, t1.term_id,
                        t2.term_web_unit_total_count,
                        t3.term_web_unit_finished_count,
                        t4.scorm_unit_sum,
                        t5.scorm_unit_id
                      from (
                        select member_id, term_id, value as provider_id from qyy_mid_online.term_learn_progress_stat_kv
                        where day= '2017-06-29' and type = 'provider_id' and category = 'common_property'
                      ) t1
                      left outer join (
                        select member_id, term_id, value as term_web_unit_total_count from qyy_mid_online.term_learn_progress_stat_kv
                        where day= '2017-06-29' and type = 'term_web_unit_total_count' and category = 'common_attribute'
                      ) t2 on (t1.member_id = t2.member_id and t1.term_id = t2.term_id)
                      left outer join (
                        select member_id, term_id, value as term_web_unit_finished_count from qyy_mid_online.term_learn_progress_stat_kv
                        where day= '2017-06-29' and type = 'term_web_unit_finished_count' and category = 'term_web_unit_finished_count'
                      ) t3 on (t1.member_id = t3.member_id and t1.term_id = t3.term_id)
                      left outer join (
                        select term_id, count(id) as scorm_unit_sum from qyy_dbload_online.s2_lesson_unit
                        where day = '2017-06-29' and content_type = 8 and (client_visible = 0 or client_visible = 1) group by term_id
                      ) t4 on (t1.term_id = t4.term_id)
                      left outer join (
                        select term_id, min(id) as scorm_unit_id from qyy_dbload_online.s2_lesson_unit
                        where day = '2017-06-29' and content_type = 8 and (client_visible = 0 or client_visible = 1) group by term_id
                      ) t5 on (t1.term_id = t5.term_id)
                  ) tt1
                  left outer join (
                      select lesson_unit_id, learner_id, view_progress from qyy_dbload_online.scorm_view_record
                      where day = '2017-06-29'
                  ) t6 on (tt1.member_id = t6.learner_id and tt1.scorm_unit_id = t6.lesson_unit_id)







select tt1.member_id, tt1.term_id, 'user_app_learn_progress',
              if(cast(tt1.term_app_unit_total_count as bigint) >= 1,
                  if(cast(tt1.term_app_unit_total_count as bigint) = 1 and cast(tt1.scorm_unit_sum as bigint) = 1,
                      ceiling(if(t6.view_progress is null, 0, t6.view_progress) * 100),
                      if(cast(tt1.term_app_unit_finished_count as bigint) >= 0,
                          if((tt1.term_app_unit_finished_count / tt1.term_app_unit_total_count) * 100 <= 0,
                              0,
                              if((tt1.term_app_unit_finished_count / tt1.term_app_unit_total_count) * 100 <= 99,
                                  ceiling((tt1.term_app_unit_finished_count / tt1.term_app_unit_total_count) * 100),
                                  if((tt1.term_app_unit_finished_count / tt1.term_app_unit_total_count) * 100 < 100,
                                      floor((tt1.term_app_unit_finished_count / tt1.term_app_unit_total_count) * 100),
                                      100
                                  )
                              )
                          ),
                          0
                      )
                  ),
                  null
              ) as user_app_learn_progress
              from
                  (
                      select t1.member_id, t1.term_id,
                        t2.term_app_unit_total_count,
                        t3.term_app_unit_finished_count,
                        t4.scorm_unit_sum,
                        t5.scorm_unit_id
                      from (
                        select member_id, term_id, value as provider_id from qyy_mid_online.term_learn_progress_stat_kv
                        where day= '2017-06-29' and type = 'provider_id' and category = 'common_property'
                      ) t1
                      left outer join (
                        select member_id, term_id, value as term_app_unit_total_count from qyy_mid_online.term_learn_progress_stat_kv
                        where day= '2017-06-29' and type = 'term_app_unit_total_count' and category = 'common_attribute'
                      ) t2 on (t1.member_id = t2.member_id and t1.term_id = t2.term_id)
                      left outer join (
                        select member_id, term_id, value as term_app_unit_finished_count from qyy_mid_online.term_learn_progress_stat_kv
                        where day= '2017-06-29' and type = 'term_app_unit_finished_count' and category = 'term_app_unit_finished_count'
                      ) t3 on (t1.member_id = t3.member_id and t1.term_id = t3.term_id)
                      left outer join (
                        select term_id, count(id) as scorm_unit_sum from qyy_dbload_online.s2_lesson_unit
                        where day = '2017-06-29' and content_type = 8 and (client_visible = 0 or client_visible = 2) group by term_id
                      ) t4 on (t1.term_id = t4.term_id)
                      left outer join (
                        select term_id, min(id) as scorm_unit_id from qyy_dbload_online.s2_lesson_unit
                        where day = '2017-06-29' and content_type = 8 and (client_visible = 0 or client_visible = 2) group by term_id
                      ) t5 on (t1.term_id = t5.term_id)
                  ) tt1
                  left outer join (
                      select lesson_unit_id, learner_id, view_progress from qyy_dbload_online.scorm_view_record
                      where day = '2017-06-29'
                  ) t6 on (tt1.member_id = t6.learner_id and tt1.scorm_unit_id = t6.lesson_unit_id)




select t1.member_id, t1.term_id, 'term_user_learn_progress',
              if(t4.term_unit_total_count is not null and t4.term_unit_total_count >= 1,
                  if(t2.user_web_learn_progress is null,
                      if(t3.user_app_learn_progress is null, 0, t3.user_app_learn_progress),
                      if(t3.user_app_learn_progress is null,
                          t2.user_web_learn_progress,
                          if(t2.user_web_learn_progress >= t3.user_app_learn_progress, t2.user_web_learn_progress, t3.user_app_learn_progress)
                      )
                  ),
                  null
              ) as term_user_learn_progress
              from
                  (
                    select member_id, term_id, value as provider_id from qyy_mid_online.term_learn_progress_stat_kv
                    where day= '2017-06-29' and type = 'provider_id' and category = 'common_property'
                  ) t1
                  left outer join (
                    select member_id, term_id, cast(value as bigint) as user_web_learn_progress from qyy_mid_online.term_learn_progress_stat_kv
                    where day= '2017-06-29' and type = 'user_web_learn_progress' and category = 'user_web_learn_progress'
                  )t2 on (t1.member_id = t2.member_id and t1.term_id = t2.term_id)
                  left outer join (
                    select member_id, term_id, cast(value as bigint) as user_app_learn_progress from qyy_mid_online.term_learn_progress_stat_kv
                    where day= '2017-06-29' and type = 'user_app_learn_progress' and category = 'user_app_learn_progress'
                  )t3 on (t1.member_id = t3.member_id and t1.term_id = t3.term_id)
                  left outer join (
                    select member_id, term_id, cast(value as bigint) as term_unit_total_count from qyy_mid_online.term_learn_progress_stat_kv
                    where day= '2017-06-29' and type = 'term_unit_total_count' and category = 'common_attribute'
                  ) t4 on (t1.member_id = t4.member_id and t1.term_id = t4.term_id)





